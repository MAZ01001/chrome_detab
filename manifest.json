{
    "manifest_version": 3,
    "name": "Chrome detab",
    "version": "1.3.1",
    "description": "Chrome extension to popup any tab or move it back to a normal window",
    "homepage_url": "https://github.com/MAZ01001/chrome_detab",
    "key": "",
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "permissions": [ "contextMenus", "storage" ],
    "background": { "service_worker": "background.js" },
    "options_ui": { "page": "options.html", "open_in_tab": false },
    "action": {
        "default_title": "Click to move all selected tabs to popups and focus currently active page",
        "default_icon": {
            "16": "img/icon-16.png",
            "32": "img/icon-32.png",
            "48": "img/icon-48.png",
            "128": "img/icon-128.png"
        }
    },
    "commands": {
        "toggle": {
            "suggested_key": { "default": "Alt + 0" },
            "description": "Popup the last active tab (and selected tabs) or move it back to the last normal window"
        },
        "new normal": {
            "suggested_key": { "default": "Alt + Shift + 0" },
            "description": "Move the last active tab (and selected tabs) or popup to a new normal window"
        }
    }
}